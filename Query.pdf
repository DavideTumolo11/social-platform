1

SELECT `users`.`username`
FROM `users`
JOIN `medias` ON `users`.`id` = `medias`.`user_id`
WHERE `medias`.`type` = 'video'
GROUP BY `users`.`username`;



2

SELECT `posts` . `id`, `posts` . `user_id`, `posts` . `title`
FROM posts
LEFT JOIN likes ON posts.id = likes.post_id
WHERE likes.post_id IS NULL;






3

SELECT `posts`.`id`, COUNT(`likes`.`post_id`) AS `number_of_likes`
FROM `posts`
LEFT JOIN `likes` ON `posts`.`id` = `likes`.`post_id`
GROUP BY `posts`.`id`
ORDER BY `number_of_likes`




4

SELECT `users`.`id`, `users`.`username`, COUNT(`medias`.`id`) AS `number_of_media`
FROM `users`
LEFT JOIN `medias` ON `users`.`id` = `medias`.`user_id`
GROUP BY `users`.`id`
ORDER BY `number_of_media`;




5

SELECT `users`.`id`, `users`.`username`, COUNT(`likes`.`post_id`) AS `total_likes`
FROM `users`
JOIN `posts` ON `users`.`id` = `posts`.`user_id`
LEFT JOIN `likes` ON `posts`.`id` = `likes`.`post_id`
GROUP BY `users`.`id`
ORDER BY `total_likes`
